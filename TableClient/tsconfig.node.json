{
  "compilerOptions": {
	"module": "CommonJS",
	"target": "ES2020",
	"outDir": "dist",
	"lib": ["ESNext"],
	"strict": true,
	"esModuleInterop": true,
	"composite": true,
	"declaration": true, // Optional, enables generation of .d.ts files
	"declarationMap": true // Optional, generates source maps for .d.ts files
  },
  "include": ["server/**/*.ts"]
}

